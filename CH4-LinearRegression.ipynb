      "source": [
        "# Finding Coefficients - Closed Form\n",
        "\n",
        "X = np.array([[1, 1], [1, 2], [1, 3], [1, 4]])\n",
        "y = np.array([2, 3, 2.5, 5])\n",
        "\n",
        "result = np.linalg.inv(X.T @ X) @ X.T @ y\n",
        "\n",
        "print(result)"
      ],
